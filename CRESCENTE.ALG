Algoritmo "crescente"


Var
   x, y : inteiro


Inicio
   Escreval("Digite dois numeros:")
   leia(x)
   leia(y)
   
   enquanto x <> y faca
     se x < y entao
        escreval("CRESCENTE")
     senao
        escreval("DECRESCENTE")
   fimenquanto
   
   escreval("Digite outros dois numeros:")



Fimalgoritmo